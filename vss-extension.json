{
  "manifestVersion": 1,
  "id": "push-builds-and-release-task",
  "name": "Push Builds and Release",
  "version": "0.3.3",
  "publisher": "HsuanLee",
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "description": "Automatically push builds and release",
  "categories": [
    "Azure Pipelines"
  ],
  "icons": {
    "default": "images/extension-icon.png"
